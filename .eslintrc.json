{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-unreachable": "off",
      "no-empty": "off",
      "no-constant-condition": "off"
    }
}
